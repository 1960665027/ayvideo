 <!--pages/mycenter/mycenter.wxml-->
<view class="wrap">
  <!--用户登录-->
    <navigator url="../information/information" class="anglesNavigator"> 
  <view class="user_logon">
    <view bindtap="bindViewTap" class="userinfo" >
      <image bindtap="change_avatarUrl_" class="userinfo-avatar" src="{{userInfo[0].avatarurl}}" background-size="cover"></image>
      <text class="userinfo-nickname">{{userInfo[0].nickname}}</text>
        <image  src='image/more.png' class='more'></image>
    </view>
    
  </view>
  </navigator>
  <view class="list_box"></view>
</view>
<view class="anglesList"> 
  <navigator url="../myvideo/myvideo" class="anglesNavigator"> 

    <view class="imgBox"> 
      <image class="tubiao" src="/imgs/video.png" mode="aspectFill"></image> 
    </view> 
    <view class="textBox"> 
      <text class="name">我的视频</text>
      <image class='jiantou_tou' src='image/jiantou_tou.png'></image>
    </view> 
    </navigator>
    
</view>
